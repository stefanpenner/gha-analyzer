{
  "name": "github-actions-profiler",
  "version": "0.1.0",
  "type": "module",
  "description": "Analyze GitHub Actions workflow performance and generate Chrome Tracing format output for Perfetto.dev visualization",
  "main": "main.mjs",
  "bin": {
    "github-actions-profiler": "./main.mjs"
  },
  "scripts": {
    "start": "node main.mjs",
    "test": "node --test test/*.test.mjs",
    "test:unit": "node --test test/unit.test.mjs",
    "test:integration": "node --test test/consolidated.test.mjs",
    "test:watch": "node --test --watch test/*.test.mjs",
    "lint": "echo 'No linter configured - consider adding ESLint'",
    "clean": "rm -f *.json trace-*.json output-*.json"
  },
  "keywords": [
    "github-actions",
    "performance",
    "profiling",
    "chrome-tracing",
    "perfetto",
    "ci-cd",
    "devtools",
    "analysis",
    "visualization",
    "workflow"
  ],
  "author": "GitHub Actions Performance Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/github-actions-profiler.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/github-actions-profiler/issues"
  },
  "homepage": "https://github.com/your-org/github-actions-profiler#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "nock": "^13.4.0",
    "undici": "^5.29.0"
  }
}
